<vector
    android:height="20px"
    android:width="520px"
    android:tint="#888888"
    android:viewportWidth="520.0"
    android:viewportHeight="20.0"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
pathData values
1. M = Move cursor to x, y position
    M0,0 or M100, 50
2. First L = Point to start drawing in x, y position
    L0, 0 or L100, 50
3. Second L = Point to finish drawing in x, y position
    L0, 0 or L100, 50
-->
<!--    Horizontal line-->
    <path
        android:fillColor="#0000FF"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="4"
        android:pathData="M10,10 L10,10 L510,10"/>

<!--    Vertical lines-->
    <path
        android:fillColor="#0000FF"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="4"
        android:pathData="  M10,0 L10,0 L10,20
                            M260,0 L260,0 L260,20
                            M140,0 L140,0 L140,20
                            M380,0 L380,0 L380,20
                            M510,0 L510,0 L510,20
                        "/>
</vector>
